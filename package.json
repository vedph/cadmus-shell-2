    "build-grp": "ng build @myrmidon/cadmus-graph-ui && ng build @myrmidon/cadmus-graph-pg",
    "build-pre": "ng build @myrmidon/cadmus-preview-ui && ng build @myrmidon/cadmus-preview-pg",
    "build-lib": "npm run build-cor && npm run build-api && npm run build-itm && npm run build-ui && npm run build-ths && npm run build-gen && npm run build-phi && npm run build-grp && npm run build-pre"
