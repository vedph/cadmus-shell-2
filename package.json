{
  "name": "cadmus-shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-cor": "ng build @myrmidon/cadmus-core",
    "build-api": "ng build @myrmidon/cadmus-api",
    "build-ui": "ng build @myrmidon/cadmus-ui && ng build @myrmidon/cadmus-state",
    "build-uip": "ng build @myrmidon/cadmus-ui-pg",
    "build-ths": "ng build @myrmidon/cadmus-profile-core && ng build @myrmidon/cadmus-thesaurus-ui && ng build @myrmidon/cadmus-thesaurus-editor && ng build @myrmidon/cadmus-thesaurus-list",
    "build-gen": "ng build @myrmidon/cadmus-part-general-ui && ng build @myrmidon/cadmus-part-general-pg",
    "build-phi": "ng build @myrmidon/cadmus-part-philology-ui && ng build @myrmidon/cadmus-part-philology-pg",
    "build-itm": "ng build @myrmidon/cadmus-item-list && ng build @myrmidon/cadmus-item-editor && ng build @myrmidon/cadmus-item-search && ng build @myrmidon/cadmus-layer-demo",
    "build-grp": "ng build @myrmidon/cadmus-graph-ui && ng build @myrmidon/cadmus-graph-pg && ng build @myrmidon/cadmus-graph-pg-ex",
    "build-pre": "ng build @myrmidon/cadmus-preview-ui && ng build @myrmidon/cadmus-preview-pg",
    "build-lib": "npm run build-cor && npm run build-api && npm run build-ui && npm run build-itm && npm run build-uip && npm run build-ths && npm run build-gen && npm run build-phi && npm run build-grp && npm run build-pre"
  },
  "overrides": {
    "d3-selection": "3.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.3",
    "@angular/cdk": "^16.1.3",
    "@angular/common": "^16.1.3",
    "@angular/compiler": "^16.1.3",
    "@angular/core": "^16.1.3",
    "@angular/forms": "^16.1.3",
    "@angular/material": "^16.1.3",
    "@angular/platform-browser": "^16.1.3",
    "@angular/platform-browser-dynamic": "^16.1.3",
    "@angular/router": "^16.1.3",
    "@auth0/angular-jwt": "^5.1.2",
    "@myrmidon/auth-jwt-admin": "2.0.0",
    "@myrmidon/auth-jwt-login": "2.0.0",
    "@myrmidon/cadmus-graph-ui-ex": "^1.0.3",
    "@myrmidon/cadmus-refs-asserted-chronotope": "2.0.0",
    "@myrmidon/cadmus-refs-asserted-ids": "^2.1.7",
    "@myrmidon/cadmus-refs-assertion": "2.0.0",
    "@myrmidon/cadmus-refs-doc-references": "^2.0.0",
    "@myrmidon/cadmus-refs-historical-date": "2.0.0",
    "@myrmidon/cadmus-refs-lookup": "^2.0.0",
    "@myrmidon/cadmus-refs-proper-name": "2.0.0",
    "@myrmidon/cadmus-text-block-view": "^2.0.0",
    "@myrmidon/ng-mat-tools": "2.0.0",
    "@myrmidon/ng-tools": "2.0.0",
    "@myrmidon/ngx-dirty-check": "^1.0.0",
    "@ngneat/effects-ng": "^3.1.3",
    "@ngneat/elf": "^2.3.2",
    "@ngneat/elf-cli-ng": "^1.0.0",
    "@ngneat/elf-devtools": "^1.3.0",
    "@ngneat/elf-entities": "^4.4.4",
    "@ngneat/elf-pagination": "^1.1.0",
    "@ngneat/elf-requests": "^1.8.0",
    "@swimlane/ngx-graph": "^8.2.1",
    "@types/diff-match-patch": "^1.0.32",
    "@types/rangy": "0.0.35",
    "diff-match-patch": "^1.0.5",
    "fast-deep-equal": "^3.1.3",
    "gravatar": "^1.8.2",
    "ngx-markdown": "^16.0.0",
    "ngx-monaco-editor": "^12.0.0",
    "rangy": "^1.3.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.2",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.3",
    "@angular/cli": "^16.1.3",
    "@angular/compiler-cli": "^16.1.3",
    "@angular/localize": "^16.1.3",
    "@types/d3-dispatch": "^3.0.2",
    "@types/d3-drag": "^3.0.2",
    "@types/d3-timer": "^3.0.0",
    "@types/jasmine": "~4.3.5",
    "@types/node": "^20.4.2",
    "jasmine-core": "~5.0.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.1.0",
    "typescript": "~5.0.4"
  }
}
